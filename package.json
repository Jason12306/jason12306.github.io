{
  "name": "vitepress-blog",
  "version": "1.0.0",
  "description": "vitepress blog",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vitepress dev docs",
    "build": "cross-env NODE_ENV=production vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "publish": "npm run docs:build && node nodejs/publish.mjs",
    "algolia:dev": "cross-env NODE_ENV=development node deploy/algolia.mjs",
    "algolia:prod": "cross-env NODE_ENV=production node deploy/algolia.mjs",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "blog",
    "vue",
    "vite",
    "vitepress"
  ],
  "author": "vanyi",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.14.9",
    "algoliasearch": "^4.24.0",
    "cross-env": "^7.0.3",
    "csvtojson": "^2.0.10",
    "lodash": "^4.17.21",
    "shelljs": "^0.8.5",
    "vitepress": "^1.3.0",
    "vue": "^3.2.40"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
